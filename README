#РАСЧЕТ ЧАСТОТ СОБСТВЕННЫХ КОЛЕБАНИЙ ВАГОНА   (1)
from math import *
c1=3.9*10**6 #Жёсткость одного комплекта рессор (н/м)
alpha=0.7 #Использование грузоподъёмности вагона (%)
Mr=25.42*1000 #Тара вагона (Кг)
Mgr=67*1000 #грузоподъёмность (Кг)
pi=3.14 #Коэффициент
Ln=5.3 #Длина неровности (м)
q=0.438*1000 #Вес необрессоренных частей, приходящих на 1-но колесо(кг)
n=8 #Число колёс
g=9.81
Mm=4.680*1000 #Вес тележки (кг)
fct=0.049 #Cтатический прогиб (м)
hc=1.85 #Высота центра тяжести кузова с грузом над уровнем рессорного подвешивания(м)
Mm=4.680*1000 #Масса тележки (кг)
mk=Mr+alpha*Mgr-2*Mm   #масса обрессоренных частей кузова вагона (кг)

c=4*c1 #суммарная жесткость рессорного подвешивания, Н/м,

Ny1=sqrt(c/mk) #Собственная частота колебаний при β = 0 (рад/с)

print "c(8)=", c
print "mk (7)=", mk
print "Ny1(1)=", Ny1


V1=Ln/(2*pi)*sqrt(c/mk) #Критическая скорость движения (м/с)
print "V1 (2)=", V1


#Наибольший прогиб рессор будет при скорости

V2=3*Ln/(4*pi)*sqrt(c/mk) #Cкорость (м/с)
print "V2 (3)=", V2


L=7.8 #База вагона (м)
l1=l2=L/2 #Половина базы вагона (м)
print "l1=", l1
print "l2=", l2


Iy=17 #Момент инерции кузова относительно оси Oy (кг*м**2)
Ny2=sqrt((2*c1*(l1**2+l2**2)-mk*g*hc)/Iy*10**9) #Частота собственных колебаний галопирования(рад/с)
print "Ny2 (5)=", Ny2


Ix=28.5 #момент инерции кузова вагона с грузом относительно оси Оx (кг*м**2)
b=L/2  #Расстояние между линиями действия сил упругости рессорных комплектов (м)
Ny3=sqrt((4*c1*b**2-mk*g*hc)/Ix*10**8)
print "Ny3 (6)=", Ny3



Etta0=0.015  #Амплитуда неровностей (м)
V=16.7  #Скорость движения (м/c)

w=2*pi*V/Ln #Частота неровностей пути
print "w=", w




Ksim=1.47*Etta0
print "Ksim (4)=", Ksim

#Частота n (Гц) и период T (c) :
n1=Ny1/(2*pi)
n2=Ny2/(2*pi)
n3=Ny3/(2*pi)
print "n1 (9)=", n1
print "n2=", n2
print "n3=", n3

T1=1/n1
T2=1/n2
T3=1/n3
print "T1=", T1
print "T2=", T2
print "T3=", T3